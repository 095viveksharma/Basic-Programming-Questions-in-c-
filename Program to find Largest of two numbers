Method 1:- Using If else
#include<iostream>
using namespace std;

int main()
{
    int a,b;
    cout<<"Enter first number ";
    cin>>a;
    cout<<endl<<"enter Second number ";
    cin>>b;
    if(a>b)
        cout<<a<<" is Greater.";
    else if(a<b)
        cout<<b<<" is Greater.";
    else
        cout<<"Both numbers are equal.";
    return 0;
}

Method 2:- Using Turnary operator
#include<iostream>
using namespace std;

int main()
{
    int a,b;
    cout<<"Enter first number ";
    cin>>a;
    cout<<endl<<"enter Second number ";
    cin>>b;
    (a>b)? cout<<a<<" is Greater." : cout<<b<<" is Greater.";
    return 0;
}
